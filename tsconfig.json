{
    "compilerOptions": {
        "target": "ES2020",
        "module": "NodeNext",
        "outDir": "dist",
        "strict": true,
        "sourceMap": true,
        "moduleResolution": "NodeNext",
        "esModuleInterop": true,
        "noImplicitAny": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*"],
}