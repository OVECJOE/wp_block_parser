{
  "name": "wp_block_parser",
  "private": true,
  "version": "0.0.1",
  "description": "An NPM library for parsing WordPress block content into JSON.",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "dev:watch": "tsx watch src/index.ts",
    "dev": "tsx src/index.ts",
    "playground": "tsx src/playground.ts",
    "format": "prettier --write 'src/**/*.{js,ts,json}'",
    "lint:all": "pnpm lint && pnpm tscCheck",
    "lint:fx": "eslint src/**/*.ts --fix",
    "lint": "eslint src/**/*.ts",
    "start": "tsc && node dist/index.js",
    "tsc": "tsc",
    "tsxCheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OVECJOE/wp_block_parser.git"
  },
  "keywords": [],
  "author": "OVECJOE <ovecjoe123@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/OVECJOE/wp_block_parser/issues"
  },
  "packageManager": "pnpm@6.32.2",
  "homepage": "https://github.com/OVECJOE/wp_block_parser#readme",
  "devDependencies": {
    "@npmcli/fs": "^3.1.0",
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.12.5",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rimraf": "5.0.5",
    "tsc": "^2.0.4",
    "tsx": "4.7.1",
    "typescript": "^5.4.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.ts": "tsc --noEmit",
    "*.{js,ts,json}": "prettier --write"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "inversify": "^6.0.2",
    "js-yaml": "^4.1.0",
    "reflect-metadata": "^0.2.2",
    "uuid": "^9.0.1"
  }
}
